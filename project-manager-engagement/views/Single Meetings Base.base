properties:
  file.name:
    displayName: Meeting
views:
  - type: table
    name: meetings
    filters:
      and:
        - file.inFolder("meetings/single")
    order:
      - file.name
      - engagement
      - date
    sort:
      - property: date
        direction: DESC
    columnSize:
      file.name: 346
      note.engagement: 212
      note.date: 171
